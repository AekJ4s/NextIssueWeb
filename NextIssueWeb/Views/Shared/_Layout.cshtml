   <script>
    $(document).on('click', '#openCreateProjectModal', function (e) {
        e.preventDefault(); // ป้องกันการทำงานปกติของปุ่ม

        console.log("Try to open popup");

        $.ajax({
            url: '@Url.Action("popup", "Project")', // ระบุชื่อ Controller และ Action ที่ถูกต้อง
            type: 'GET',
            success: function (result) {
                console.log("Success open popup");
                $('#createProjectModalBody').html(result);
                $('#createProjectModal').modal('show');
            },
            error: function (xhr, status, error) {
                if (xhr.status === 401) {
                    // ผู้ใช้ไม่ได้รับอนุญาต ให้ redirect ไปที่หน้า Login
                    window.location.href = '@Url.Action("LoginPage", "Login")';
                } else {
